IDVinculoUsuario IDVinculoUsuario, @IDUser IDUsuario, 0 IMF, (SELECT TOP 1 IMGEmpresa FROM #TEMP) IMGEmpresa, '' Justificado, '' MF, @MeSRef MesReferencia, '' OBS, PrimeiroNome,
'00:00:00' SaidaManha, '00:00:00' SaidaTarde, 'Não Bateu' Situacao, -1 SituacaoJustificativa, (SELECT TOP 1 SituacaoN FROM #TEMP) SituacaoN, 0 TotHorasDiarias, VU.matricula,VU.IsencaoPonto
FROM TBVinculoUsuario VU
JOIN TBUsuario U ON (VU.IdUsuario = u.IdUsuario)
JOIN TBEmpresa E ON (E.IdEmpresa = VU.IdEmpresa)
JOIN TBSetor S ON (S.IdSetor = VU.IdSetor)
JOIN TBDiasAno A ON (A.IdEmpresa = VU.IdEmpresa)
JOIN TBEntidade ENT ON (ENT.IdEntidade = VU.IdEntidade)
JOIN dbo.TBMes MES ON (MES.IDMes = @MeSRef)
WHERE DATEPART(YYYY,DTDiasAno) = @AnoRef 
AND DATEPART(MM,DTDiasAno) = @MeSRef 
AND (@IDUser = CASE WHEN 0 = @IDUser THEN @IDUser ELSE VU.IDusuario END) 
AND (VU.IDEmpresa = @IDEmpresa) 
AND (@IDsetor = CASE WHEN 0 = @IDsetor THEN @IDsetor ELSE VU.IDsetor END) 
AND DTDiasAno NOT IN (SELECT DTFrequencia FROM #TEMP)
AND DTDiasAno &lt; GETDATE()+1
ORDER BY DTFrequencia</CommandText>
