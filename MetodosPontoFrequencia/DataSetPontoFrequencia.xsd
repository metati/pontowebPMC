                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT HD.AnoReferencia, HD.DSEmpresa, HD.DSEntidade, HD.DSMes, HD.DSSetor, HD.DSUsuario, HD.DTFrequencia, HD.DataFrequencia, 
	  HD.DescontoTotalJornada, HD.EntradaManha, HD.EntradaTarde, HD.FotoUsuario, 
                     isNull(CONVERT(VARCHAR(5),CONVERT(TIME,E.DataHoraEntrada)), HD.HoraEntradaManha) HoraEntradaManha, 
	  isNull(CONVERT(VARCHAR(5),CONVERT(TIME,E.DataHoraEntrada)), HD.HoraEntradaTarde) HoraEntradaTarde, 
	  isNull(CONVERT(VARCHAR(5),CONVERT(TIME,E.DataHorasSaida)), HD.HoraSaidaManha) HoraSaidaManha, 
	  isNull(CONVERT(VARCHAR(5),CONVERT(TIME,E.DataHorasSaida)), HD.HoraSaidaTarde) HoraSaidaTarde, 
                    HD.HorasDia, HD.IDClienteSIG, HD.IDEmpresa, HD.IDFrequencia, HD.IDRegimeHora, HD.IDSetor, 
	  HD.IDTPJustificativa, HD.IDVinculoUsuario, HD.IDusuario, HD.IMF, HD.IMGEmpresa, HD.Justificado, HD.MF, HD.MesReferencia, HD.OBS, 
	  HD.PrimeiroNome, HD.SaidaManha, HD.SaidaTarde, HD.Situacao, HD.SituacaoJustificativa, HD.SituacaoN, isNull(DATEDIFF(HOUR,E.DataHoraEntrada,E.DataHorasSaida), TotHorasDiarias) TotHorasDiarias, HD.matricula,HD.IsencaoPonto		
INTO #TEMP
FROM vwHorasDia HD
JOIN TBRegimeHora RH ON (RH.IDRegimeHora = HD.IDRegimeHora)
LEFT JOIN TbRegimeHorario_Escala  E WITH(NOLOCK) ON (HD.DTFrequencia = E.DataEscala AND HD.IdVinculoUsuario = E.IdVinculoUsuario)
WHERE 
RH.RegimePlantonista = 1
AND (@IDUser = CASE WHEN 0 = @IDUser THEN @IDUser ELSE HD.IDusuario END) 
AND (HD.AnoReferencia = @AnoRef) 
AND (@MeSRef = CASE WHEN 0 = @MeSRef THEN @MeSRef ELSE HD.MesReferencia END) 
AND (IDEmpresa = @IDEmpresa) 
AND (@IDsetor = CASE WHEN 0 = @IDsetor THEN @IDsetor ELSE HD.IDsetor END) 
AND (HD.IDFrequencia &lt;&gt; 0 OR E.DataEscala IS NOT NULL)
AND HD.DTFrequencia &lt; GETDATE()+1

SELECT * FROM #TEMP
UNION 
SELECT DATEPART(YYYY,DTDiasAno) AnoReferencia,DSEmpresa, DSEntidade, DSMes, 
DSSetor,DSUsuario, DTDiasAno DTFrequencia, CONVERT(VARCHAR(12),DTDiasAno,103) DataFrequencia, DescontoTotalJornada,
'00:00:00' EntradaManha, '00:00:00' EntradaTarde, (SELECT TOP 1 FotoUsuario FROM #TEMP) FotoUsuario, '00:00' HoraEntradaManha, '00:00' HoraEntradaTarde,
